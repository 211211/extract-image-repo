{
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "overrides": [
        {
            "files": ["*.json", ".prettierrc", ".babelrc"],
            "options": { "parser": "json" }
        }
    ]
}
