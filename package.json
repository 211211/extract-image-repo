{
  "name": "extract-image-repo",
  "version": "0.0.1",
  "description": "Allows to extract an image from a timestamp of video via url",
  "main": "build/main.js",
  "scripts": {
    "build": "npm run tsoa && npm run compile",
    "tsoa": "./node_modules/.bin/tsoa routes && tsoa swagger",
    "swagger": "./node_modules/.bin/tsoa swagger",
    "compile": "tsc -p .",
    "watch": "tsc -w",
    "start": "tsc-watch --onSuccess \"node ./build/main.js\"",
    "main": "cross-env JWT_SECRET_KEY=test node ./build/main.js",
    "main:production": "cross-env JWT_SECRET_KEY=test NODE_ENV=production node ./build/main.js",
    "main:debug": "cross-env JWT_SECRET_KEY=test NODE_ENV=debug node ./main.js",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "keywords": [],
  "author": "Quan Nguyen <admin@nguyenhongquan.com>",
  "license": "Closed Source - Redistribution is prohibited",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/method-override": "^0.0.31",
    "@types/node": "^14.0.14",
    "@types/redis": "^2.8.31",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "find-config": "^1.0.0",
    "fs-extra": "^9.0.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rfc6902": "^3.0.4",
    "shortid": "^2.2.15",
    "swagger-ui-dist": "^3.23.9",
    "tsoa": "^3.2.1"
  }
}
